{
  "git": {
    "commitMessage": "chore: release v${version}",
    "tagName": "v${version}"
  },
  "github": {
    "release": true,
    "releaseName": "v${version}"
  },
  "npm": {
    "release": true
  },
  "plugins": {
    "./dist/index.mjs": {
      "disable": false,
      "changelogFile": "./CHANGELOG.md"
    }
  }
}
